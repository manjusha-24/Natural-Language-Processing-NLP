    {
      "cell_type": "code",
      "source": [
        "from sklearn.feature_extraction.text import CountVectorizer\n",
        "reviews_df_clean = reviews_df['verified_reviews'].apply(message_cleaning)\n"
      ],
      "metadata": {
        "id": "G0768y24kIM6"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def message_cleaning(message):\n",
        "    # Handle potential float values (e.g., NaN) by converting them to strings.\n",
        "    # Check if message is either a float or np.nan before converting to string\n",
        "    if isinstance(message, float) or isinstance(message, type(np.nan)):  # Handle np.nan\n",
        "        message = str(message)  # Convert to string if it is\n",
        "\n",
        "    # If message is NaN, return empty string\n",
        "    if pd.isnull(message) or message == 'nan':  # Explicitly check for 'nan' string\n",
        "        return ''\n",
        "\n",
        "    Test_punc_removed = [char for char in message if char not in string.punctuation]\n",
        "    Test_punc_removed_join = ''.join(Test_punc_removed)\n",
        "    Test_punc_removed_join_clean = [word for word in Test_punc_removed_join.split() if word.lower() not in stopwords.words('english')]\n",
        "    return Test_punc_removed_join_clean\n",
        "\n",
        "# ... (rest of the code) ...\n",
        "\n",
        "# Replace NaN values with empty strings before vectorization\n",
        "reviews_df['verified_reviews'] = reviews_df['verified_reviews'].fillna('')\n",
        "\n",
        "vectorizer = CountVectorizer(analyzer=message_cleaning)\n",
        "reviews_countvectorizer = vectorizer.fit_transform(reviews_df['verified_reviews'])\n"
      ],
      "metadata": {
        "id": "Xr8XR9_2Idfv"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "print(vectorizer.get_feature_names_out())"
      ],
      "metadata": {
        "id": "_djKe7iiv9Sa",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "bfdd2b11-6073-48d3-96c5-fa6bdb1ed164"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "['072318' '1' '10' ... '😬' '😳' '🤓']\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
